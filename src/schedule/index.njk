---
title: Schedule
subtitle: All sessions will be streamed live during the event, and will be available after the event on the Chrome Developers YouTube channel.
---

{% extends 'layouts/schedule/index.njk' %}

{% block content %}
    {%- set utilsCSS = "/_includes/utils.css" -%}
    {% css page, utilsCSS %}

    {%- set gridCss = "/_includes/grid.css" -%}
    {% css page, gridCss %}

    {%- set cardCss = "/_includes/card/style.css" -%}
    {% css page, cardCss %}

    {%- set contentCSS = "/_includes/content.css" -%}
    {% css page, contentCSS %}

    {%- set carouselCss = "/_includes/carousel/style.css" -%}
    {% css page, carouselCss %}

    {%- set speakerCSS = "/_includes/speakers.css" -%}
    {% css page, speakerCSS %}

    <section class="{% className moduleCSS, 'section' %}">
        <div>
            <div class="{% className scheduleCss, 'tz-switcher' %}">
                {% include "timezone-toggle/index.njk" %}
            </div>

            <div class="{% className gridCss, 'grid' %}" cols="2">
                {%- for event in collections.featuredEvents -%}
                    <article class="{% className cardCss, 'card' %}" media="1">
                        <a href="{{conf.path}}{{ event.data.link }}">
                            <div class="{% className utilsCSS, 'aspect-ratio' %} {% className cardCss, 'card-media' %}" style="--aspect-ratio: calc(16/9)">
                                <img class="{% className utilsCSS, 'media-fill' %}"
                                     src="confboxAsset(/assets/16-9-placeholder.png)"
                                     alt="{{ event.data.title }}">
                            </div>

                            <div class="{% className cardCss, 'content' %}">
                                <div>
                                    <span class="{% className cardCss, 'tag' %}">{{ event.data.prettyType }}</span>

                                    <h2 class="{% className cardCss, 'title' %}">{{ event.data.title }}</h2>
                                </div>

                                <span class="{% className cardCss, 'footer' %}">
                                DAY {{ event.data.day }}, {% confDate event.data.start, 'hh:mm' %} â€“ {% confDate event.data.end, 'hh:mm' %}
                            </span>
                            </div>
                        </a>
                    </article>
                {%- endfor -%}
            </div>
        </div>
    </section>

    <section class="{% className moduleCSS, 'section' %}">
        <h3 class="{% className contentCSS, 'section-heading' %}">Workshops</h3>

        <span class="{% className scheduleCss, 'subheading-container' %}">
            <p class="{% className contentCSS, 'section-subheading' %}">Hands-on workshops with leading experts</p>

            <a href="{{conf.path}}schedule/workshops/">View all</a>
        </span>

        {% include "workshop-carousel/index.njk" %}
    </section>

    <section class="{% className moduleCSS, 'section' %}">
        <h3 class="{% className contentCSS, 'section-heading' %}">Office hours</h3>

        <span class="{% className scheduleCss, 'subheading-container' %}">
            <p class="{% className contentCSS, 'section-subheading' %}">Private office hours with Chrome experts</p>
            <a href="{{conf.path}}schedule/office-hours/">View all</a>
        </span>

        {% include "office-hours-carousel/index.njk" %}
    </section>

    <section class="{% className moduleCSS, 'section' %}">
        <h3 class="{% className contentCSS, 'section-heading' %}">Learning lounge</h3>

        <span class="{% className scheduleCss, 'subheading-container' %}">
            <p class="{% className contentCSS, 'section-subheading' %}">Learn about all of the latest updates and tools</p>
            <a href="{{conf.path}}schedule/learning-lounge/">View all</a>
        </span>

        {% include "learning-lounge-carousel/index.njk" %}
    </section>
{% endblock %}
